#
# Windows 7 64bit in VirtualBox
#

assert_spec 64bit

set flavor   "win7_64"

set use_vbox4 1
set use_vbox5 0

set use_rumpfs 1
# Write overlay only into ram
set use_ram_fs 1
# However read initial overlay from disk
set use_overlay_from_disk 1

set use_usb 1
set use_ps2 [have_spec ps2]

source ${genode_dir}/repos/ports/run/vbox_win.inc
